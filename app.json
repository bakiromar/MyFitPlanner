{
  "expo": {
    "name": "MyFitPlanner",
    "description": "A workout app.",
    "slug": "fitness-planner",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "version": "5.0.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/app-icon.png",
    "loading": {
      "icon": "./assets/icons/loading-icon.png",
      "hideExponentText": false
    },
    "packagerOpts": {
      "assetExts": ["ttf", "mp4"]
    },
    "ios": {
      "bundleIdentifier": "com.techatservice.myfitplanner"
    },
	  "android": {
      "package": "com.techatservice.myfitplanner",
      "config": {
        "googleMaps": {
          "apiKey": "Your Google Places API Here"
        }
      },
	    "versionCode": 5,
	    "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    }
  }
}
